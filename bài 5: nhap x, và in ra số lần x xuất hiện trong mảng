#include <stdio.h>

int main() {
    int a[] = {1,2,3,4,5,6,7,8,9,9, 8,7,6,5,4,3,2,1,10,1,2,3,4,23,55,6,5,3,2,5,6,7,3,2,3,5,575,3,2135,0,1,2,3,5,6,8,3,21,3,6,6,34,234,6,67,345,7,35,234} ;
    int x, ddz = 0 ,n = sizeof (a)/ sizeof(a[0]);
    printf("nhap vao so can check: ");
    scanf("%d", &x);
    for(int i = 0; i < n; i++)
    {
        if (a[i] == x){
        ddz++ ;}
    }
    printf("so lan so %d xuat hien trong mang la: ", x);
    printf("%d", ddz);
    return 0;
}
