    printf("nhập số: ", c);
    scanf("%d", &c);
    printf("nhập số: ", d);
    scanf("%d", &d);
    printf("nhập số: ", e);
    scanf("%d", &e);
    printf("nhập số: ", f);
    scanf("%d", &f);
