#include <stdio.h>
void hoanvi(int a[], int k,int n){
    if(n == k){
       for(int i = 0; i < n; i++){
        printf("%d", a[i]);
       }printf("\n");
    }else {
       for(int i = k; i < n; i++){
        int temp = a[k];
        a[k] = a[i];
        a[i] = temp;

        hoanvi(a, k+1, n);

        temp = a[k];
        a[k] = a[i];
        a[i] = temp;
       }
    }
}
int main (){
   int n;
   int a[200];
   printf("nhap n: ");
   scanf("%d", &n);
   for(int i = 0; i < n; i++){
    a[i] = i + 1;
   }
    hoanvi(a, 0, n);
    return 0;
   }
